from selenium import webdriver
from selenium.webdriver.common.by import By
import time
# driver=webdriver.Chrome()
# driver.get('https://www.vip.com/')
# driver.maximize_window()
# driver.find_element(By.CLASS_NAME,'c-search-input').send_keys('python')
# driver.find_element(By.CLASS_NAME,'c-search-button').click()
# time.sleep(5)

class Myspider():
    def __init__(self):
        self.driver=webdriver.Chrome()
        self.page=self.driver.get('https://www.vip.com/')


    def data_get(self):
        self.driver.maximize_window()
        self.driver.find_element(By.CLASS_NAME,'c-search-input').send_keys('python')
        time.sleep(3)
        self.driver.find_element(By.CLASS_NAME, 'c-search-button').click()
        time.sleep(10)
        divs=self.driver.find_elements(By.XPATH,'//section[@id="J_searchCatList"]/div')

        for i in  divs[1:]:
          book_name=i.find_element(By.XPATH,'./a/div[2]/div[2]').text
          print(book_name)
vip=Myspider()
vip.data_get()
